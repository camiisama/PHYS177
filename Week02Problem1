def f(x):
    return 2*x**3 - 6*x + 3.

a = -1.
b = 1.
N = 10

h = (b - a) / float(N)
I = f(a) + f(b)
for k in range(1,N):
	I = I + 2. * f(a + k*h)

I = I * 0.5 * h
print  'Trapezoid Rule:',I

def f(x):
    return 2*x**3 - 6*x + 3.

a = -1.
b = 1.
N = 10

h = (b - a) / float(N)
I = f(a) + f(b)

for k in range(1,N/2 + 1):
    I = I + 2 * f(a + 2*k*h)
    
for k in range(1,N/2):
    I = I + 4 * f(a + (2*k - 1) * h)

I = h/3 * I
print  'Simpson Rule:',I
